{
    "name": "shadow-bean-co-migration",
    "version": "1.0.0",
    "description": "Migration scripts for Shadow Bean Co",
    "scripts": {
        "migrate:db": "node migrate-db.js",
        "migrate:db:dry": "node migrate-db.js --dry-run",
        "migrate:images": "node migrate-images.js",
        "migrate:images:dry": "node migrate-images.js --dry-run",
        "migrate:all": "npm run migrate:db && npm run migrate:images"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^3.x",
        "@aws-sdk/client-s3": "^3.x",
        "@aws-sdk/util-dynamodb": "^3.x",
        "@supabase/supabase-js": "^2.x",
        "pg": "^8.x"
    }
}